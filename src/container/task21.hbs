<div>
  <main>
    <div class='card'>
      <div class='card-body'>
        {{#with heading}}
          <div class='card'>
            <div class='card-body'>
              {{#with main}}
                <h1>{{title}}</h1>
               
                <p>{{text}}</p>
                              
                {{#with button_text}}
                  <button
                    type='button'
                    class='btn btn-primary'
                  >{{solid}}</button>
                  <br />
                  <br />
                  <button
                    type='button'
                    class='btn btn-outline-primary'
                  >{{outline}}</button>
                {{/with}}
              {{/with}}
            </div>
          </div>
          <br />
          <div class='card'>
            <div class='card-body'>
              {{#with second}}
                {{>title name=title big=true}}
                
                <p>{{text}}</p>
                
                {{#with button_text}}
                  <button
                    type='button'
                    class='btn btn-secondary'
                  >{{solid}}</button>
                  <br />
                  <br />
                  <button
                    type='button'
                    class='btn btn-outline-secondary'
                  >{{outline}}</button>
                {{/with}}
              {{/with}}
            </div>
          </div>
          <br />
          <div class='card'>
            <div class='card-body'>
              {{#with subblock}}
                
                {{> title name=title big=false}}
                
                <p>{{text}}</p>
                
                {{#with button_text}}
                  <button
                    type='button'
                    class='btn btn-dark'
                  >{{solid}}</button>
                  <br />
                  <br />
                  <button
                    type='button'
                    class='btn btn-outline-dark'
                  >{{outline}}</button>
                {{/with}}
              {{/with}}
            </div>
          </div>
        {{/with}}
      </div>
    </div>
    <br />
    <div class='card'>
      <div class='card-body'>
        {{#with nested_display}}
          <div class='display-1'>{{title}}</div>
          <br />
          <div class='card'>
            <div class='card-body'>
                {{#with children_display}}
          <div class='display-4'>{{title}}</div>      
          <p>{{text}}</p>
          
          <button
                    type='button'
                    class='btn btn-outline-warning'
                  >{{button.text}}</button>
                {{/with}}
            </div>
                </div>
            <br>

            <h6>{{header_text}}</h6>
            <br>
            <button
                    type='button'
                    class='btn btn-info'
                  >{{button.text}}</button>
        {{/with}}
        <br>
      </div>
    </div>
        <div class='card'>
            <div class='card-body'>
            {{#with paragraph}}
        <p>{{title}}</p>
        <p>{{text}}</p>
                {{#with button}}
                    <a href="{{url}}" class="btn btn-link">{{text}}</a>
                {{/with}}
                <br>
            <div class='card'>
            <div class='card-body'>
                {{#each list_name}}
                <ul> 
                    <li><a href="{{url}}" class="btn btn-link">{{text}}</a></li>
                </ul>
                {{/each}}
            </div>
            </div>    
            {{/with}}
    </div>
    </div>
  </main>
</div>