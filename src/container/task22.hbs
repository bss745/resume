<div>
  <main>
    {{#with page_header}}
      <h1 class='text-center'>{{title}}</h1>
      <p class='text-center'>{{description}}</p>
      <hr />
    {{/with}}
    {{#with section}}
      {{#with about_us}}
        {{#with title_list}}
          <div class='display-6 text-end'>{{main}}</div>
          <br />
          <h2 class='text-end'>{{sub}}</h2>
        {{/with}}
        <br />
        <button
          type='button'
          class='btn btn-outline-info'
        >{{button.text}}</button>
        <br />
        <br>
      {{/with}}

    <div class='card'>
    <div class='card-body'>
      
        {{#with advantage}}
                    
            <div class='display-5'>{{title}}</div>
            <br />
            {{#with efficiency}}
              <div class='card'>
                <div class='card-body'>
                  <div class='display-4'>{{title}}</div>
                  <br />
                  <h6>{{sub_title}}</h6>
                  <p>{{description}}</p>
                  <button
                    type='button'
                    class='btn btn-outline-warning'
                  >{{button}}</button>
                </div>
              </div>
            {{/with}}
            <br />
            {{#with innovation}}
              <div class='card'>
                <div class='card-body'>
                  <div
                    class='display-4 text-center'
                  >{{title}}</div>
                  <br />
                  <h6 class='text-center'>{{sub_title}}</h6>
                  <p class='text-center'>{{description}}</p>
                  <button
                    type='button'
                    class='btn btn-outline-warning'
                  >{{button}}</button>
                </div>
              </div>
            {{/with}}
            <br />
            {{#with collaboration}}
              <div class='card'>
                <div class='card-body'>
                  <div
                    class='display-4 text-end'
                  >{{title}}</div>
                  <br />
                  <h6 class='text-end'>{{sub_title}}</h6>
                  <p class='text-end'>{{description}}</p>
                  <button
                    type='button'
                    class='btn btn-outline-warning'
                  >{{button}}</button>
                </div>
              </div>
            {{/with}}
        {{/with}}
        {{#with agreement}}
            <div class='display-5'>{{title}}</div>
        <br />
            <div class='card'>
            <div class='card-body'>
                {{#with info}}
                <h5>{{sub_title}}</h5>
                <hr>
                    {{#with detail}}
                    <p>{{text}}</p>
                    <button
                    type='button'
                    class='btn btn-success'
                  >{{button}}</button>
                    {{/with}}
                {{/with}}
            </div>
            </div>
            
            <br />
        {{/with}}
               

        {{/with}}
    </div>
    </div>
  </main>
</div>