            {{#each navigation}}
                <a href="{{href}}" class="btn btn-link col-3">{{text}}</a>
                </a>
            {{/each}} 
        </div>
        <div>
            {{#each socialMedia}}
                <a href="{{href}}" class="btn btn-ordinary-dark col-3">{{text}}</a>
                </a>
            {{/each}} 
        </div>
        <div>
            {{#each partners}}
                <a href="{{href}}" class="btn btn-ordinary-info col-3">{{text}}</a>
                </a>
            {{/each}} 
        </div> --}}
            

        
        <div class="row">
                   
                <div class="col-3 list-group">
                    {{#each navigation}}
                    <div><a href="{{href}}" class="btn btn-link">{{text}}</a></div>
                    {{/each}}
                </div>
                    
                    
                <div class="col-3 list-group">
                    {{#each socialMedia}}
                    <div><a href="{{href}}" class="btn btn-dark">{{text}}</a></div>
                    {{/each}}
                </div>
                    
                    
                <div class="col-6 list-group">
                    {{#each partners}}
                    <div><a href="{{href}}" class="btn btn-info">{{text}}</a></div>
                    {{/each}}
                </div>
                    
        </div>
         


    
    {{/with}}
  {{!-- </div> --}}
</main>